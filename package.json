{
	"name": "librarykit",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test:mikro": "./test-mikro.sh",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"clean-cache": "rm -rf .svelte-kit node_modules/.vite",
		"migration:create": "node --no-warnings=ExperimentalWarning --loader ts-node/esm ./node_modules/.bin/mikro-orm migration:create --config=./src/mikro-orm.config.test.ts",
		"migration:up": "node --no-warnings=ExperimentalWarning --loader ts-node/esm ./node_modules/.bin/mikro-orm migration:up --config=./src/mikro-orm.config.test.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@mikro-orm/cli": "^6.4.16",
		"@mikro-orm/migrations": "^6.4.16",
		"@playwright/test": "^1.49.1",
		"@storybook/addon-essentials": "^8.6.14",
		"@storybook/addon-interactions": "^8.6.14",
		"@storybook/addon-svelte-csf": "^5.0.0-next.0",
		"@storybook/blocks": "^8.6.14",
		"@storybook/svelte": "^8.6.14",
		"@storybook/sveltekit": "^8.6.14",
		"@storybook/test": "^8.6.14",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.21.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.1.7",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.4",
		"@types/node": "^18",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"jsdom": "^26.0.0",
		"mdsvex": "^0.12.3",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"storybook": "^8.6.14",
		"svelte": "^5.33.10",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.1.7",
		"ts-node": "^10.9.2",
		"tsx": "^4.19.4",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.6",
		"vitest": "^3.0.0"
	},
	"dependencies": {
		"@mikro-orm/core": "^6.4.16",
		"@mikro-orm/postgresql": "^6.4.16",
		"@mikro-orm/reflection": "^6.4.16",
		"@tanstack/svelte-query": "^5.76.0",
		"@trpc/client": "^11.1.2",
		"@trpc/server": "^11.1.2",
		"html5-qrcode": "^2.3.8",
		"trpc-svelte-query": "^3.0.3",
		"zod": "^3.25.1"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
